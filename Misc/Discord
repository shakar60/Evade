    Local.InviteCode = "3jtkr4kWKb"
    Local.DiscordAPI = "https://discord.com/api/v10/invites/" .. Local.InviteCode .. "?with_counts=true&with_expiration=true"

    Local.Response = WindUI.cloneref(game:GetService("HttpService")):JSONDecode(
        WindUI.Creator.Request({
            Url = Local.DiscordAPI,
            Method = "GET",
            Headers = {
                ["User-Agent"] = "WindUI/Example",
                ["Accept"] = "application/json"
            }
        }).Body
    )

    if Local.Response and Local.Response.guild then
        Local.Tabs.Discord:Section({Title = "Join our Discord server!", TextSize = 20, Box = true, BoxBorder = true})

        Local.Tabs.Discord:Paragraph({
            Title = tostring(Local.Response.guild.name),
            Desc  = "The server of the creator of this script.",
            Image = "https://cdn.discordapp.com/icons/" 
                .. Local.Response.guild.id .. "/" 
                .. Local.Response.guild.icon .. ".png?size=1024",
            Thumbnail = "https://cdn.discordapp.com/banners/"
                .. Local.Response.guild.id
                .. "/" 
                .. (Local.Response.guild.banner or "") .. ".png?size=512",

            ImageSize = 48,
            Buttons = {
                {
                    Title = "Copy link",
                    Icon = "link",
                    Callback = function()
                        setclipboard("https://discord.gg/" .. Local.InviteCode)
                    end
                }
            }
        })
    end
